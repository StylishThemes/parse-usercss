{
  "name": "parse-usercss",
  "version": "0.1.0",
  "description": "Parse usercss style used by Stylus extension",
  "license": "MIT",
  "repository": "StylishThemes/parse-usercss",
  "author": {
    "name": "StylishThemes",
    "url": "https://github.com/StylishThemes"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "css",
    "color",
    "usercss",
    "parser"
  ],
  "devDependencies": {
    "ava": "*",
    "deepmerge": "^2.0.1",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "capitalized-comments": 0,
      "comma-dangle": 0,
      "curly": 0,
      "default-case": 0,
      "import/prefer-default-export": 0,
      "linebreak-style": 0,
      "new-cap": 0,
      "no-mixed-operators": 0,
      "operator-linebreak": 0,
      "padded-blocks": 0,
      "quote-props": 0,
      "quotes": [2, "single", {"avoidEscape": true}],
      "unicorn/prefer-starts-ends-with": 0
    }
  }
}
